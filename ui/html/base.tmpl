    <script>
 






        function notifyModel(message, type = 'success') {

            Swal.fire({
                position: 'top-end',
                icon: type,
                title: message,
                text: 'Something went wrong!',
                showConfirmButton: false,
                timer: 1500
            })
        }



     

        {{ with .Error }}
        Swal.fire({
                            position: 'top-end',
                            icon: "error",
                            text: "{{.}}",
                            showConfirmButton: false,
                            timer: 4000,
                            showCloseButton: true
                            })
        {{ end }}

      
        {{ with .Flash }}
        

        Swal.fire({
                            position: 'top-end',
                            icon: "success",
                            text: "{{.}}",
                            showConfirmButton: false,
                            timer: 2000,
                            showCloseButton: true
                            })
        {{ end }}

        {{ with .Warning }}
        Swal.fire({
                            position: 'top-end',
                            icon: "warning",
                            text: "{{.}}",
                            showConfirmButton: false,
                            timer: 2500,
                            showCloseButton: true
                            })
         {{ end }}
    </script>




