{{define "title"}}
Delete Batch query
{{end}}

{{define "content"}}
 

    <div class="row p-2">
        <div class="col">
          <div class="card ">
            <div class="card-header">
              <p class="h5">Delete Batch Query 
                </p>
            </div>
            <div class="card-body ">

<table class="table table-bordered table-striped">
    <tbody>
        <tr>
            <td>Server</td>
            <td>{{.BatchQuery.Server.Name}}</td>
        </tr>

        <tr>
            <td>Query</td>
            <td>{{.BatchQuery.RunningSql.RunningNow}}</td>
        </tr>

       
     

      

        <tr>
            <td>Status</td>
            <td>{{.BatchQuery.Status}}</td>
        </tr>

   
 
    </tbody>
</table>

<form method="post" action="/batchquery/delete">
        <input type="hidden" name="csrf_token" value="{{.CSRFToken}}">
          <input type="hidden" name="queryid" value="{{.BatchQuery.RunningSql.ID}}">
    <button type="submit" class="btn btn-danger">Confirm</button>

</form>
</div>
</div>
</div>
</div>

{{end}}