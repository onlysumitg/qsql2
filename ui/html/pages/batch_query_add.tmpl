  <!-- quert start-->

  <div id="leftone" class=""  >


    <div class="row p-2">
      <div class="col">
        <div class="card ">
          <div class="card-header">

            {{template "batch_query_form" .}}



          </div>
          <div class="card-body"> 

      <editor name="sql" editor-id="editorB" :key="id"  :content="contentB" v-on:change-content="changeContentB" v-on:run-sql="runSQL">
      </editor>


    </div></div></div></div>
  </div>
  <!-- quert end-->


  <!-- RESULT start-->




  <div id="rightone"   style="overflow:hidden;">
    <div class="row p-2">
      <div class="col">
        <div class="card ">
       
          <div class="card-body"> 

    <div class=" ">
    {{template "query_result" .}}
  </div>
 
     </div> </div> </div> </div> </div>

  <!-- RESULT end-->

</div>


