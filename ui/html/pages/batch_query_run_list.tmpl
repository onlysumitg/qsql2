{{define "title"}}
Batch queries
{{end}}

{{define "content"}}
 
 <div class="row p-2">
  <div class="col">
    <div class="card ">
      <div class="card-header">
        <p class="h5">Results for Batch job
       </p>
      </div>
      <div class="card-body">
 
 <table id="batchqueryrunlist"  class="table   table-borderless table-responsive-sm table-striped    ">
  <thead class="thead-dark">
     <tr>
      <th>Run#</th>
 
       <th>CreatedAt</th>
        
       
       <th>Options </th>
       
      </tr>
    </thead>
    <tbody>
      
      {{if .BatchQuertRuns}}
        {{range .BatchQuertRuns}}
        <tr>
          <td>{{.RunCounter}}</td>
 
             <td>{{.CreatedAt | humanDate }}</td>
          
            
            <td>
              <a class="btn btn-primary" href='/batchquery/runs/result/{{.ID}}'  >Results</a>

            </td>

        </tr>
        {{end}}
        {{end}}
          </tbody>
        </table>
  </div>
</div> </div> </div>
{{end}}


{{define "aftercontent"}}

<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.13.1/css/jquery.dataTables.css">
<script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.13.1/js/jquery.dataTables.js"></script>

<script>
  
    $(document).ready( function () {
      $('#batchqueryrunlist').DataTable(
        {
              "language": {
                "emptyTable": "No results found"
              }
          }
      );
  } );
</script>
{{end}}